ductionSection{\n\n  width: 95%;\n  margin-left: 5%;\n}\n\n#IntroductionSection h1{\n\n margin-top: 0%\n}\n#headerlist{\n\n\n  display: none\n}\n#stackMenuIcon\n{\n\n\n  display:block;\n  float: right;\n  font-size: 150%;\n  border: none;\n    background: #FFFC31;\n    color: black;\n}\n\n#Kalaam{\n\n  float: left;\n    text-decoration: none;\n    margin-left: 4%;\n    font-size: 163%;\n    margin-top: 1%;\n    color: black;\n}\n\n#LogoandTitle{\n\nposition: absolute;\n    display: block;\n    width: 65%;\n\n\n}\n#KalamLogo{\n\n  width: 60%;\n  margin-top: 5%\n   \n}\n\n#headerlistMobile{\n\n       display: inline-grid;\n    position: absolute;\n    left: 0%;\n    width: 100%;\n    background-color: #FFFC31;\n    line-height: 40px;\n    top: 86%;\n    padding: 0%;\n    \n}\n\n\n#headerlistMobile li{\n\n      display: inline-block;\n    font-size: 120%;\n    margin-top: 1%\n\n}\n#headerlistMobile a{\n\n  text-decoration: none;\n   cursor:pointer; \n   color: rgb(12, 12, 12)\n}\n\n#headerlistMobile a:hover{\n\n \n   color: rgb(214, 200, 0)\n}\n::-webkit-input-placeholder {\ntext-align: center;\nvertical-align: middle;\n\nline-height: 500px;}\n\n:-moz-placeholder { /* Firefox 18- */\ntext-align: center;\nvertical-align: middle;\n\nline-height: 500px;}\n\n::-moz-placeholder {  /* Firefox 19+ */\ntext-align: center;\nvertical-align: middle;\n\nline-height: 500px;}\n\n:-ms-input-placeholder {  \ntext-align: center;\nvertical-align: middle;\n\nline-height: 500px;}\n\n#button2{\n\n\n    margin-left: 3%\n  }\n\n  #buttons button{\n\n   border-radius: 2em;\n   border: solid 0.3px black;\n   font-size: 100%;\n   width: 40%;\n   height: 43px;\n   background-color: white;\n   color: black\n  }\n\n  span.cursor {\n      \n     text-align: left;\n    margin-top: 5%;\n    width: 3px;\n    display: -webkit-box;\n    display: -ms-flexbox;\n    display: flex;\n    height: 38px;\n    margin-left: 1%;\n     background-color: rgb(0, 0, 0);\n      animation: cursorBlink 1s infinite;\n    }\n\n}\n@media only screen\nand (min-width : 768px)\nand (max-width : 1024px) {\n\n\n\n#headerlist li{\n\nmargin-left: 0%\n}\n\n\n span.cursor {\n      \n     text-align: left;\n    margin-top: 4%;\n    width: 3px;\n    display: -webkit-box;\n    display: -ms-flexbox;\n    display: flex;\n    height: 38px;\n    margin-left: 1%;\n     background-color: rgb(0, 0, 0);\n      animation: cursorBlink 1s infinite;\n    }\n\n#LogoSection{\n\n\n  width: 40%\n}\n\n#IntroductionSection{\n\n\n  width: 60%\n}\n\n#KalamLogo{\n\n  width: 50%\n}\n\n#buttons button{\n\n  width: 35%;\n  margin-top: 10%\n}\n\n}\n\n  \n\n</style>"]}]}